{
  "name": "reference-storefront",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lib:build": "ng build storefront && cd projects/storefront && yarn scss-bundle -e ./src/styles/styles.scss -d ../../dist/storefront/styles.scss -p . --includePaths ./src/styles",
    "lib:watch": "concurrently -n build,scss \"ng build storefront --watch\" \"cd projects/storefront && yarn scss-bundle -c scss-bundle.config.json\" ",
    "lib:extract-translations": "cd projects/storefront && yarn ngx-translate-extract --input ./src --output ./src/i18n/en.json --clean --sort --format namespaced-json --format-indentation \"  \" -m _",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "analyze-prod-bundle": "ng build --prod --statsJson=true && webpack-bundle-analyzer dist/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-types": "gql-gen --config codegen/codegen.yml",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run reference-storefront:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0-rc.2",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/pwa": "^0.800.2",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.0.0-rc.1",
    "@vendure/storefront": "^0.1.1",
    "compression": "^1.7.4",
    "core-js": "^3.1.3",
    "express": "^4.17.3",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.2",
    "@angular-devkit/build-ng-packagr": "~0.800.2",
    "@angular/cli": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.24",
    "codelyzer": "~5.1.0",
    "concurrently": "^4.1.0",
    "graphql-code-generator": "0.18.0",
    "graphql-codegen-add": "0.18.0",
    "graphql-codegen-time": "0.18.0",
    "graphql-codegen-typescript-client": "0.18.0",
    "graphql-codegen-typescript-common": "0.18.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.3.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.4",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0"
  }
}

